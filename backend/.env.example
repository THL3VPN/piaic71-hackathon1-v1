# Backend Service Configuration for PIAIC71-Hackathon1

# Server Configuration
BACKEND_HOST=localhost
BACKEND_PORT=8000
LOG_LEVEL=INFO

# GitHub Pages CORS Configuration
GITHUB_PAGES_ORIGIN=https://your-username.github.io

# Database Configuration (PostgreSQL/Neon)
NEON_DATABASE_URL=postgresql://username:password@ep-xxx.region.aws.neon.tech/dbname?sslmode=require
DATABASE_USER=your_db_user
DATABASE_PASSWORD=your_db_password
DATABASE_HOST=ep-xxx.region.aws.neon.tech
DATABASE_PORT=5432
DATABASE_NAME=your_db_name

# Database Connection Pool Settings
DATABASE_POOL_SIZE=5
DATABASE_POOL_TIMEOUT=30
DATABASE_POOL_RECYCLE=3600

# Qdrant Vector Database Configuration
QDRANT_URL=http://qdrant-host:6333
QDRANT_HOST=qdrant-host
QDRANT_PORT=6333
QDRANT_API_KEY=your_qdrant_api_key

# Qdrant Vector Configuration
VECTOR_SIZE=1536
DISTANCE_METRIC=Cosine

# SQLAlchemy Configuration
SQLALCHEMY_ECHO=false  # Set to true for SQL query logging

# Application-specific settings
MAX_DOCUMENT_SIZE=5000000  # 5MB in bytes
ALLOWED_FILE_EXTENSIONS=pdf,docx,txt,md

# Timeout configurations (in milliseconds)
STARTUP_TIMEOUT=10000
REQUEST_TIMEOUT=30000